    </Extension>
    <Extension Id="com.my.localserver">
      <DispatchInfo>
        <Resources>
          <MainPath>./localServer.html</MainPath>
            <CEFCommandLine>
              <Parameter>--enable-nodejs</Parameter>
              <Parameter>--mixed-context</Parameter>
            </CEFCommandLine>
